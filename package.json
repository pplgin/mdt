{
  "name": "airclass-classroom",
  "version": "1.0.0",
  "description": "国际老师端课堂项目",
  "main": "dist/app.js",
  "scripts": {
    "dll:dev": "cross-env  NODE_ENV=development webpack --config build/webpack.config.dll.js --mode development",
    "dll": "cross-env webpack --config build/webpack.config.dll.js --mode production",
    "start": "cross-env BUILD_TYPE=test NODE_ENV=development webpack serve --config ./build/webpack.config.dev.js --progress",
    "release:dev": "cross-env  NODE_ENV=production BUILD_TYPE=test webpack --config build/webpack.config.prod.js",
    "release:uat": "cross-env NODE_ENV=production BUILD_TYPE=uat webpack --config build/webpack.config.prod.js",
    "release": "cross-env  NODE_ENV=production BUILD_TYPE=prod webpack --config build/webpack.config.prod.js",
    "check": "tsc -w",
    "analyzer": "cross-env NODE_ENV=production DEBUG=true webpack --config build/webpack.config.analyzer.js",
    "fat": "npm run dll && npm run release:dev",
    "uat": "npm run dll && npm run release:uat",
    "pro": "npm run dll && npm run release",
    "test-t": "dts-gen -m svga.lite -d svga.lite -o",
    "prepare": "husky install"
  },
  "author": "",
  "license": "ISC",
